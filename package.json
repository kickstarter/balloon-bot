{
  "name": "balloon-bot",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:oliverswitzer/balloon-bot.git",
  "author": "Oliver Switzer <oliver@kickstarter.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "start": "webpack --watch --progress --mode development --config=web/javascripts/webpack.config.js",
    "build": "webpack --mode production",
    "webpack:deploy": "webpack --config=web/javascripts/webpack.config.js -p",
    "heroku-postbuild": "yarn webpack:deploy"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.ts"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "jooks": "^1.0.0",
    "moment": "^2.24.0",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.17",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  }
}
